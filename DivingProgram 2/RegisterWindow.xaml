        mc:Ignorable="d"
        Title="RegisterWindow" Height="450" Width="800">
    <Grid>
        <PasswordBox x:Name="amountOfDives" HorizontalAlignment="Left" Margin="309,236,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="lestName" HorizontalAlignment="Left" Margin="309,155,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="firstName" HorizontalAlignment="Left" Margin="309,121,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <DatePicker x:Name="userBirthDate" HorizontalAlignment="Left" Margin="309,193,0,0" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="232,123,0,0" TextWrapping="Wrap" Text="First Name:" VerticalAlignment="Top"/>

    </Grid>
